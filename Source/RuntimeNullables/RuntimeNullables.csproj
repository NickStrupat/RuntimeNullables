<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageTags>Null Check, NRT, Nullable, ILWeaving, Fody, Cecil, AOP</PackageTags>
    <Description>Automatic null check injection for runtime C# 8+ Nullable Reference Type (NRT) contract validation.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Singulink/RuntimeNullables</PackageProjectUrl>
    <Company>Singulink</Company>
    <Authors>Mike Marynowski</Authors>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Copyright (c) 2020 Singulink</Copyright>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>key.snk</AssemblyOriginatorKeyFile>
    <DelaySign>false</DelaySign>
  </PropertyGroup>

  <ItemGroup>
    <AdditionalFiles Include="stylecop.json" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FodyPackaging" Version="6.2.0" />
  </ItemGroup>

</Project>
